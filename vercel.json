{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/compress",
      "dest": "app.py"
    },
    {
      "src": "/merge",
      "dest": "app.py"
    },
    {
      "src": "/suggestions",
      "dest": "app.py"
    },
    {
      "src": "/forum/posts(.*)",
      "dest": "app.py"
    },
    {
      "src": "/download/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/cleanup/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/health",
      "dest": "app.py"
    },
    {
      "src": "/test-merge-route",
      "dest": "app.py"
    },
    {
      "src": "/",
      "dest": "app.py"
    },
    {
      "src": "/(.*\\.(html|css|js|json|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
